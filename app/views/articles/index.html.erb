<h3>all articles listing</h3>
<p>
  <%= link_to "new articles create",new_article_path %>
</p>
<table>
  <tr>
    <th>title</th>
    <th>description</th>
  </tr>
  <% @articles.each do |article| %>
    <tr>
      <td><%= article.title %></td>
      <td><%= article.description %></td>
      <td><%= link_to "Edit",edit_article_path(article) %></td>
      <td><%= link_to "Show",article_path(article) %></td>
    </tr>
    <% end %>
</table>
